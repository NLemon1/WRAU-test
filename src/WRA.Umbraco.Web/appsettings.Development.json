{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      }
    }
  },
  "MessagingSettings": {
    "EndpointSettings": [
      {
        "Enabled": false,
        "EndpointType": "Topic",
        "EntityName": "Member",
        "EntityEventName": "MemberEvent",
        "RateLimitSettings": {
          "Enabled": true,
          "RateLimit": 5,
          "RateLimitPeriod": 60
        },
        "ConcurrencyLimitSettings": {
          "Enabled": true,
          "ConcurrencyLimit": 1
        }
      },
      {
        "Enabled": false,
        "EndpointType": "Topic",
        "EntityName": "Product",
        "EntityEventName": "ProductEvent",
        "RateLimitSettings": {
          "Enabled": true,
          "RateLimit": 5,
          "RateLimitPeriod": 60
        },
        "ConcurrencyLimitSettings": {
          "Enabled": true,
          "ConcurrencyLimit": 1
        }
      }
    ]
  }
}
{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Licenses": {
      "Umbraco.Commerce": "LJXCQ-EVMSZ-VTPPG-XOJRJ"
    },
    "Commerce": {
      "StorefrontAPi": {
        "Enabled": true,
        "ApiKey": "BtwRaJBwxCYe7eBKnbC7"
      }
    },
    "Deploy": {
      "Settings": {
        "TransferFormsAsContent": true,
        "AllowMembersDeploymentOperations": "Transfer",
        "TransferMemberGroupsAsContent": true
      }
    },
    "CMS": {
      "Global": {
        "Id": "a3d722eb-f749-43bf-8e8b-57d0c548ee30",
        "UseHttps": true,
        "NoNodesViewPath": "~/umbraco/UmbracoWebsite/Deploy/NoNodes.cshtml",
        "ReservedPaths": "~/app_plugins/,~/install/,~/mini-profiler-resources/,~/umbraco/,~/error/",
        "SanitizeTinyMce": true,
        "Smtp": {
          "Host": "smtp.postmarkapp.com",
          "From": "WRA@lightburn.co",
          "Port": 587,
          "SecureSocketOptions": "Auto",
          "Username": "Umbraco__CMS__Global__Smtp__Username",
          "Password": "Umbraco__CMS__Global__Smtp__Password"
        },
        "MemberPassword": {
          "RequiredLength": 10,
          "RequireNonLetterOrDigit": false,
          "RequireDigit": false,
          "RequireLowercase": false,
          "RequireUppercase": false,
          "HashAlgorithmType": "HMACSHA256",
          "MaxFailedAccessAttemptsBeforeLockout": 5
        }
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "Error404Collection": [
          {
            "Culture": "default",
            "ContentXPath": "//error404[@nodeName='404']"
          }
        ]
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing",
        "ModelsNamespace": "WRA.Umbraco.Models"
      },
      "Unattended": {
        "UpgradeUnattended": true
      }
    }
  },
  "ConnectionStrings": {
    "AzureServiceBus": "ConnectionString__AzureServiceBus"
  },
  "SubscriptionServiceSettings": {
    "MemberSubscriptionSettings": {
      "AzureUrl": "SubscriptionServiceSettings__MemberSubscriptionSettings__AzureUrl",
      "BaseUrl": "https://localhost:44336",
      "SubscriptionName": "website-prod-member-subscription",
      "TopicName": "website-prod-member"
    },
    "ProductSubscriptionSettings": {
      "AzureUrl": "SubscriptionServiceSettings__ProductSubscriptionSettings__AzureUrl",
      "BaseUrl": "https://localhost:44336",
      "SubscriptionName": "website-prod-product-subscription",
      "TopicName": "website-prod-product"
    }
  }
